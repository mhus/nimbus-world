# Nimbus Client Configuration
# Copy this file to .env and fill in your values

# Server Configuration
#VITE_SERVER_WEBSOCKET_URL=ws://localhost:3011
#VITE_SERVER_API_URL=http://localhost:3011
VITE_SERVER_WEBSOCKET_URL=ws://localhost:9042/ws
VITE_SERVER_API_URL=http://localhost:9042

VITE_EXIT_URL=http://localhost:3002/controls/dev-login.html

# Logging (optional)
VITE_LOG_LEVEL=DEBUG
VITE_LOG_TO_CONSOLE=true

# Splash Screen (optional)
# Set to false to disable splash screen on engine start
VITE_SHOW_SPLASH_SCREEN=true

# Note: Environment variables must be prefixed with VITE_ for Vite to expose them to the browser
