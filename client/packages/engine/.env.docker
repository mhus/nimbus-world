# Nimbus Client Configuration
# Copy this file to .env and fill in your values

# Server Configuration
VITE_SERVER_API_URL=http://localhost:9042

# Logging (optional)
VITE_LOG_LEVEL=DEBUG
VITE_LOG_TO_CONSOLE=true

# Splash Screen (optional)
# Set to false to disable splash screen on engine start
VITE_SHOW_SPLASH_SCREEN=true
